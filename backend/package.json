{
  "name": "@parcel-lab-shopify-app/backend",
  "version": "1.0.0",
  "description": "",
  "author": "Pascal Garber <pascal@artandcode.studio>",
  "license": "AGPL",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "reinstall": "YARN_CHECKSUM_BEHAVIOR=update yarn install",
    "prebuild": "rimraf dist",
    "build": "npm run lint && nest build",
    "build:frontend": "cd frontend && touch yarn.lock && yarn install && yarn run build",
    "npm:build:frontend": "cd frontend && npm install && npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "watch": "npm run start:debug",
    "typecheck": "tsc --project tsconfig.build.json --noEmit",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx --ignore-pattern '*.spec.ts' --fix && npm run typecheck",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "npm:link:deps": "npm link nest-shopify --private"
  },
  "url": "https://github.com/ArtCodeStudio/parcel-lab-shopify-app",
  "repository": {
    "type": "git",
    "url": "git@github.com:ArtCodeStudio/parcel-lab-shopify-app.git"
  },
  "dependencies": {
    "@nest-middlewares/express-session": "^6.0.1",
    "@nestjs/cli": "^7.5.5",
    "@nestjs/common": "^7.6.12",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.12",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.12",
    "@nestjs/platform-socket.io": "^7.6.12",
    "@nestjs/typeorm": "^7.1.5",
    "@nestjs/websockets": "^7.6.12",
    "@parcel-lab-shopify-app/frontend": "workspace:frontend",
    "@types/async": "^3.2.5",
    "@types/cache-manager": "^3.4.0",
    "@types/cache-manager-ioredis": "^2.0.1",
    "@types/connect-redis": "^0.0.16",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
