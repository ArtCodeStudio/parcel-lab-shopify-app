  "scripts": {
    "reinstall": "YARN_CHECKSUM_BEHAVIOR=update yarn install",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:frontend": "cd frontend && touch yarn.lock && yarn install && yarn run build",
    "npm:build:frontend": "cd frontend && npm install && npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "watch": "npm run start:debug",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "npm:link:deps": "npm link nest-shopify --private"
  },
  "url": "https://github.com/ArtCodeStudio/parcel-lab-shopify-app",
  "repository": {
    "type": "git",
    "url": "git@github.com:ArtCodeStudio/parcel-lab-shopify-app.git"
