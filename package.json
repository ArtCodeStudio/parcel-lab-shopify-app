{
  "name": "parcel-lab-shopify-app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=14.0.0",
    "yarn": ">=2.0.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build && yarn run build:frontend",
    "build:frontend": "cd frontend && yarn run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.4.4",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.4.4",
    "@nestjs/typeorm": "^7.1.4",
    "@types/cache-manager-ioredis": "^2.0.1",
    "@types/ioredis": "^4.17.4",
    "@types/node-sass": "^4.11.1",
    "@yarnpkg/pnpify": "^2.2.1",
    "bootstrap": "^4.5.2",
    "cache-manager": "^3.4.0",
    "cache-manager-ioredis": "^2.1.0",
    "connect-redis": "^5.0.0",
    "deepmerge": "^4.2.2",
    "express-session": "^1.17.1",
    "find-root": "^1.1.0",
    "ioredis": "^4.17.3",
    "mongodb": "^3.6.2",
    "mongoose": "^5.10.5",
    "nest-shopify": "^7.0.0-beta.3",
    "node-sass": "^4.14.1",
    "node-sass-middleware": "^0.11.0",
    "passport": "^0.4.1",
    "pug": "^3.0.0",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "typeorm": "^0.2.26"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/connect-redis": "^0.0.14",
    "@types/express": "^4.17.3",
    "@types/express-session": "^1.17.0",
    "@types/find-root": "^1.1.2",
    "@types/jest": "26.0.10",
    "@types/node": "^13.9.1",
    "@types/node-sass-middleware": "^0.0.31",
    "@types/passport": "^1.0.4",
    "@types/redis": "^2.8.27",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "eslint": "7.7.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "26.4.2",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "resolutions": {
    "nest-shopify": "portal:../nestjs-shopify-module"
  }
}
